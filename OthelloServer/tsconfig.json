{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "lib": [ "es5", "es2015.symbol", "es2015.symbol.wellknown", "dom" ],
    "module": "es2015",
    "moduleResolution": "node",
    "paths": { "*": [ "types/*" ] },
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "types": [ "webpack-env" ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
